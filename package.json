{
  "name": "k6-to-junit",
  "version": "0.8.1",
  "description": "tool to convert k6 output to junit xml",
  "main": "dist/index.js",
  "bin": "dist/command.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "node dist/command.js",
    "format": "./node_modules/.bin/prettier --write \"src/**\"",
    "lint": "./node_modules/.bin/eslint \"src/**\"",
    "build": "tsc",
    "example": "tsc && docker run -v C:\\Users\\Matt\\Code\\git\\k6tojunit:/k6 --name kgTest --rm loadimpact/k6 run --out json=k6/exampleResults.json /k6/test/example.k6.js | npm start -- stdin --out junit.xml"
  },
  "keywords": [
    "k6",
    "junit",
    "report"
  ],
  "homepage": "https://github.com/Mattihew/k6-to-junit",
  "bugs": "https://github.com/Mattihew/k6-to-junit/issues",
  "repository": "github:Mattihew/k6-to-junit",
  "author": "Matt Rayner",
  "license": "MIT",
  "dependencies": {
    "@microsoft/ts-command-line": "^4.3.2",
    "tslib": "^1.10.0",
    "xmlbuilder": "^13.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  }
}
