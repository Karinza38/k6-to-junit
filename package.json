{
  "name": "k6-to-junit",
  "version": "0.7.0",
  "description": "tool to convert k6 output to junit xml",
  "main": "dist/index.js",
  "bin": "dist/command.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "node dist/command.js",
    "lint": "./node_modules/.bin/eslint \"src/**\"",
    "build": "tsc",
    "example": "tsc && docker run -v C:\\Users\\Matt\\Code\\git\\k6tojunit:/k6 --name kgTest --rm loadimpact/k6 run --out json=k6/exampleResults.json /k6/test/example.k6.js | npm start -- stdin --out junit.xml"
  },
  "keywords": [
    "k6",
    "junit",
    "report"
  ],
  "homepage": "https://github.com/Mattihew/k6-to-junit",
  "bugs": "https://github.com/Mattihew/k6-to-junit/issues",
  "repository": "github:Mattihew/k6-to-junit",
  "author": "Matt Rayner",
  "license": "MIT",
  "dependencies": {
    "@microsoft/ts-command-line": "^4.2.6",
    "xmlbuilder": "^13.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.6.8",
    "@types/xmlbuilder": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
